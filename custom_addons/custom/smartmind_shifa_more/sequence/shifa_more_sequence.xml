<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="sequence_consent" model="ir.sequence">
            <field name="name">Consent</field>
            <field name="code">sm.shifa.consent</field>
            <field name="prefix">con</field>
            <field name="padding">4</field>
        </record>
        <record id="seq_sm_medical_report" model="ir.sequence">
            <field name="name">Medical Report</field>
            <field name="code">sm.medical.report</field>
            <field name="prefix">Rept#</field>
            <field name="padding">4</field>
            <field name="company_id" eval="True"/>
        </record>
        <record id="sequence_multidisciplinary_team_meeting" model="ir.sequence">
            <field name="name">Multidisciplinary Team Meeting</field>
            <field name="code">sm.shifa.multidisciplinary.team.meeting</field>
            <field name="prefix">MTM</field>
            <field name="padding">4</field>
        </record>

        <record id="seq_sm_shifa_nurse_assessment" model="ir.sequence">
            <field name="name">Nurse Assessment Main</field>
            <field name="code">sm.shifa.nurse.assessment.main</field>
            <field name="prefix">NA/%(day)s-%(month)s-%(year)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="True"/>
        </record>

        <record id="seq_sm_shifa_nurse_assessment_followup" model="ir.sequence">
            <field name="name">Nurse Assessment Followup</field>
            <field name="code">sm.shifa.nurse.assessment.followup</field>
            <field name="prefix">NA_F/%(day)s-%(month)s-%(year)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="True"/>
        </record>

        <record id="seq_sm_shifa_physician_assessment" model="ir.sequence">
            <field name="name">Physician Assessment Main</field>
            <field name="code">sm.shifa.physician.assessment.main</field>
            <field name="prefix">PhA/%(day)s-%(month)s-%(year)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="True"/>
        </record>

        <record id="seq_sm_shifa_physician_assessment_followup" model="ir.sequence">
            <field name="name">Physician Assessment Followup</field>
            <field name="code">sm.shifa.physician.assessment.followup</field>
            <field name="prefix">PhA_F/%(day)s-%(month)s-%(year)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="True"/>
        </record>
        <record id="seq_sm_shifa_complaints" model="ir.sequence">
            <field name="name">Complaints</field>
            <field name="code">sm.shifa.complaints</field>
            <field name="prefix">Com</field>
            <field name="padding">4</field>
            <field name="company_id" eval="True"/>
        </record>
        
    </data>
</odoo>